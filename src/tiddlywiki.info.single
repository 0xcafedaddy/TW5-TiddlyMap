{
  "plugins": [
    "tiddlywiki/github-fork-ribbon",
    "tiddlywiki/markdown",
    "tiddlywiki/highlight",
    "felixhayashi/topstoryview",
    "felixhayashi/hotzone",
    "felixhayashi/vis",
    "felixhayashi/tiddlymap",
    "danielo/contextPlugin"
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "languages": [
    "en-US",
    "de-DE"
  ],
  "build": {
    "index": ["--rendertiddler","$:/core/save/all","index.html","text/plain"]
  },
  "config": {
    "retain-original-tiddler-path": true
  }
}
